{
    "display_information": {
        "name": "Demo Builder"
    },
    "features": {
        "app_home": {
            "home_tab_enabled": true,
            "messages_tab_enabled": false,
            "messages_tab_read_only_enabled": false
        },
        "bot_user": {
            "display_name": "Demo Builder",
            "always_online": true
        },
        "shortcuts": [
            {
                "name": "Show Auth",
                "type": "global",
                "callback_id": "shortcut_show_auth",
                "description": "Show Auth State"
            },
            {
                "name": "Login",
                "type": "global",
                "callback_id": "shortcut_demo_login",
                "description": "Login to Auth0"
            },
            {
                "name": "List",
                "type": "global",
                "callback_id": "shortcut_demo_list",
                "description": "List all Demos"
            },
            {
                "name": "Create",
                "type": "global",
                "callback_id": "shortcut_demo_create",
                "description": "Create a Demo"
            }
        ],
        "slash_commands": [
            {
                "command": "/demos",
                "description": "interact with demos",
                "usage_hint": "[showauth, login, create, list]",
                "should_escape": false
            }
        ]
    },
    "oauth_config": {
        "redirect_urls": [
            "https://127.0.0.1/redirect"
        ],
        "scopes": {
            "user": [
                "users:read",
                "users:read.email"
            ],
            "bot": [
                "chat:write",
                "commands",
                "im:history",
                "im:read",
                "im:write",
                "users:read.email",
                "users:read"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "bot_events": [
                "app_home_opened",
                "message.im"
            ]
        },
        "interactivity": {
            "is_enabled": true
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": true,
        "token_rotation_enabled": false
    }
}